<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <div>Create a username:</div>
  <%= f.text_field :handle %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirm %>
  <%= f.password_field :password_confirm %>
  <%= f.submit 'Submit' %>
<% end %>
